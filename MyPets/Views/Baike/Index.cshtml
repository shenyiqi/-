            }

        .index_top {
            background: #9C3;
        }

        p {
            line-height: 16px;
        }

        h4 {
            font-size: 20px;
            font-weight: bold;
        }

        .dog_tit {
            border-bottom: 3px solid #9C3;
            position: relative;
            height: 54px;
        }

        .dog_tit_r {
            font: 16px/44px "Microsoft YaHei";
            color: #999;
            position: absolute;
            right: 0;
            bottom: 0;
        }

        .dog_tit h2 {
            font: 24px/44px "Microsoft YaHei";
            color: #fff;
            width: 156px;
            height: 44px;
            background: #9C3;
            text-align: center;
            position: absolute;
            line-height: 44px;
            left: 37px;
            bottom: 0;
        }
        	a{text-decoration: none;color: #000;}
			.hot-pet{width: 100%;}
			.pet-box{width: 1200px;margin: 0 auto;overflow: hidden;margin-bottom:20px;}
			.pet-box .box-top{overflow: hidden;}
			.pet-box .box-top h2{font-size: 24px;font-weight:normal;width: 150px;float: left;}
